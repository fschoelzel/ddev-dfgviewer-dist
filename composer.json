{
	"repositories": {
        "dfgviewer": {
            "type": "path",
            "url": "public/typo3conf/ext/dfg-viewer"
        },
        "kitodo-presentation": {
            "type": "path",
            "url": "public/typo3conf/ext/kitodo-presentation"
        },
		"slub-digitalcollections": {
            "type": "path",
            "url": "public/typo3conf/ext/slub_digitalcollections"
        },
		"0": {
			"type": "composer",
			"url": "https://composer.typo3.org/"
		}
	},
	"name": "typo3/cms-base-distribution",
	"description": "TYPO3 CMS Base Distribution",
	"license": "GPL-2.0-or-later",
	"config": {
		"allow-plugins": {
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		},
		"platform": {
			"php": "8.2"
		}
	},
	"require": {
		"helhum/typo3-console": "^8.2",
		"typo3/minimal": "^12.4",
		"typo3/cms-about": "^12.4",
		"typo3/cms-adminpanel": "^12.4",
		"typo3/cms-belog": "^12.4",
		"typo3/cms-beuser": "^12.4",
		"typo3/cms-felogin": "^12.4",
		"typo3/cms-fluid-styled-content": "^12.4",
		"typo3/cms-form": "^12.4",
		"typo3/cms-impexp": "^12.4",
		"typo3/cms-info": "^12.4",
		"typo3/cms-redirects": "^12.4",
		"typo3/cms-reports": "^12.4",
		"typo3/cms-rte-ckeditor": "^12.4",
		"typo3/cms-setup": "^12.4",
		"typo3/cms-seo": "^12.4",
		"typo3/cms-sys-note": "^12.4",
		"typo3/cms-t3editor": "^12.4",
		"typo3/cms-tstemplate": "^12.4",
		"typo3/cms-viewpage": "^12.4",
		"kitodo/presentation": "@dev",
		"slub/dfgviewer": "@dev"
	},
    "minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"typo3-cms-scripts": [
			"typo3 install:fixfolderstructure"
		],
		"post-autoload-dump": [
			"@typo3-cms-scripts"
		]
	}
}
